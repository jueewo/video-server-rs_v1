{
  "name": "video-server-ui",
  "version": "1.0.0",
  "description": "UI assets for video-server-rs",
  "private": true,
  "type": "module",
  "scripts": {
    "build:css": "tailwindcss --input ./static/css/input.css --output ./static/css/tailwind.css --minify",
    "watch:css": "tailwindcss --input ./static/css/input.css --output ./static/css/tailwind.css --watch",
    "dev": "npm run watch:css"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.18",
    "@tailwindcss/postcss": "^4.1.18",
    "daisyui": "^5.5.17",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18"
  }
}
